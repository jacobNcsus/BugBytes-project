CREATE TRIGGER RESTOCK_PRODUCT_UPDATE BEFORE UPDATE ON PRODUCTS 
	FOR EACH ROW 
	BEGIN 
	IF (NEW.QUANTITY_IN_STOCK <= 5) THEN 
		SET NEW.QUANTITY_IN_STOCK = NEW.QUANTITY_IN_STOCK+30; 
	END IF; 
	END  

